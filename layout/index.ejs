<main>
  <div class="site-posts">
    <div class="post-list">
      <% page.posts.sort('date', -1).each((item) => { %>
        <div class="post-item">
          <div class="post-header">
            <a href="<%- url_for(item.path) %>">
              <% if(item.title !== "") { %>
              <span><%- item.title %></span>
              <% } else { %>
                <span>no-title</span>
              <% } %>
            </a>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
</main>
